  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@repo/store": "^1.0.0",
    "clsx": "^2.1.1",
    "esbuild": "0.25.10",
    "firebase": "^12.5.0",
